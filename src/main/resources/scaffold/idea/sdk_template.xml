				<root type="simple" url="jar://{path}/frameworks/libs/air/gamepad.swc!/" />
				<root type="simple" url="jar://{path}/frameworks/libs/air/crosspromotion.swc!/" />
				<root type="simple" url="jar://{path}/frameworks/libs/air/servicemonitor.swc!/" />
				<root type="simple" url="jar://{path}/frameworks/libs/air/applicationupdater.swc!/" />
				<root type="simple" url="jar://{path}/frameworks/libs/air/applicationupdater_ui.swc!/" />
			</root>
		</classPath>
		<javadocPath>
			<root type="composite" />

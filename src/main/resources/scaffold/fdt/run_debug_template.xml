<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<launchConfiguration type="com.powerflasher.fdt.ui.MobileApplicationGroup">
	<stringAttribute key="ADL_DESCRIPTOR_FILE" value="{app}"/>
	<stringAttribute key="ADL_SCREENSIZE" value="{screenSize}"/>
	<booleanAttribute key="ADVANCED_TELEMTRY" value="false"/>
	<booleanAttribute key="AIR_ANDROID_SHARED_RUNTIME" value="true"/>
	<intAttribute key="AIR_VIEWER_CONF" value="1"/>
	<stringAttribute key="APP_ARGUMENTS" value="{adlArguments}"/>
	<stringAttribute key="ARGUMENTS" value="-static-link-runtime-shared-libraries=true&#10;-target-player={playerVersion}&#10;-library-path+=&quot;{flexSDK}/frameworks/locale/en_US&quot;&#10;-default-size=550,400"/>
	<booleanAttribute key="CHECK_FOR_ANDROID_DEVICES" value="false"/>
	<listAttribute key="COMPILER_CONSTANTS">
		{constants}
	</listAttribute>
	<stringAttribute key="COMPILER_TECHNOLOGY" value="ASC"/>
	<booleanAttribute key="CREATE_APP_ROOT" value="false"/>
	<booleanAttribute key="CUSTOMIZE_ADL_ARGS" value="true"/>
	<stringAttribute key="CUSTOM_ADL_CMD_LINE" value="${nodebug} ${profile} ${screensize} ${anesdir} ${desc} ${rootdir} ${appargs}"/>
	<intAttribute key="DEBUG_VIA" value="0"/>
	<booleanAttribute key="DEINSTALL_TARGET" value="true"/>
	<booleanAttribute key="DISABLE_COMPILE_PROCESS" value="true"/>
	<booleanAttribute key="FORCE_FRESH_COMPILATION" value="false"/>
	<booleanAttribute key="FORCE_INCLUDE_TEST_RESOURCES" value="false"/>
	<booleanAttribute key="INCLUDE_TEST_RESOURCES" value="false"/>
	<booleanAttribute key="INSTALL_APPLICATION" value="true"/>
	<stringAttribute key="INSTALL_TARGET" value="installManually"/>
	<booleanAttribute key="KILL_ADB_BEFORE_INSTALL" value="false"/>
	<booleanAttribute key="KILL_ADB_ON_STOP_DEBUG" value="false"/>
	<stringAttribute key="LAUNCHER_DEPENDENCIES" value="{gradleAssembleTask}&#10;[Self]"/>
	<intAttribute key="LAUNCH_METHOD" value="0"/>
	<stringAttribute key="MAIN_CLASS" value="{mainClass}"/>
	<stringAttribute key="MOBILE_PRESET" value="Apple iPad1/2"/>
	<intAttribute key="PACKAGE_METHOD" value="0"/>
	<booleanAttribute key="PASS_CP" value="true"/>
	<booleanAttribute key="PASS_DEFINES" value="true"/>
	<booleanAttribute key="PASS_EXTERNS" value="false"/>
	<booleanAttribute key="PASS_MAIN_CLASS" value="true"/>
	<booleanAttribute key="PASS_MANIFESTS" value="true"/>
	<booleanAttribute key="PASS_RSLS" value="false"/>
	<booleanAttribute key="PASS_SWCS" value="true"/>
	<stringAttribute key="PLATFORM_SDK_PATH" value="{sdk}"/>
	<stringAttribute key="POST_COMPILE_ANT" value=""/>
	<stringAttribute key="PRE_COMPILE_ANT" value=""/>
	<stringAttribute key="PROJECT_NAME" value="{projectName}"/>
	<booleanAttribute key="SAMPLER" value="{sampler}"/>
	<booleanAttribute key="START_DEBUGGER" value="true"/>
	<stringAttribute key="START_SWF_WITH" value="AIR Debug Launcher"/>
	<stringAttribute key="TARGET_PLATFORM" value="{platform}"/>
	<stringAttribute key="TARGET_PLATFORM_CONFIG" value="{profileName}"/>
	<booleanAttribute key="USE_PROJECT_COMPILER_ARGUMENTS" value="false"/>
	<listAttribute key="org.eclipse.debug.core.MAPPED_RESOURCE_PATHS">
		<listEntry value="/{projectName}"/>
	</listAttribute>
	<listAttribute key="org.eclipse.debug.core.MAPPED_RESOURCE_TYPES">
		<listEntry value="4"/>
	</listAttribute>
</launchConfiguration>
